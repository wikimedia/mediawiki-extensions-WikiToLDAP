{
	"root": true,
	"extends": [
		"wikimedia/client-es5",
		"wikimedia/jquery",
		"wikimedia/mediawiki"
	],
	"rules": {
		"one-var": "warn",
		"vars-on-top": "warn",
		"no-jquery/no-global-selector": "warn",
		"quotes": "warn",
		"no-unused-vars": "warn",
		"compat/compat": "warn",
		"no-mixed-spaces-and-tabs": "warn"
	}
}
