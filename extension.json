	},
	"MessagesDirs": {
		"WikiToLDAP": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "WikiToLDAP/resources"
	},
	"SpecialPages": {
		"MigrateUser": "MediaWiki\\Extension\\WikiToLDAP\\Special"
	},
	"GroupPermissions": {
		"wikitoldap-in-progress": {
			"migrate-from-ldap": true
		}
	},
