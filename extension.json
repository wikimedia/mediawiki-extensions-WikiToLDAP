{
	"name": "WikiToLDAP",
	"author": [
		"Mark A. Hershberger"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikiToLDAP",
	"descriptionmsg": "wikitoldap-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.34.0"
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\WikiToLDAP\\": "includes/"
	},
	"Hooks": {
		"PostLoginRedirect":
		"MediaWiki\\Extension\\WikiToLDAP\\Hook::onPostLoginRedirect"
	},
	"MessagesDirs": {
		"WikiToLDAP": [
			"i18n"
		]
	},
	"ResourceFileModulePaths": {
		"localBasePath": "resources",
		"remoteExtPath": "WikiToLDAP/resources"
	},
	"ResourceModules": {
		"ext.wikiToLDAP": {
			"dependencies": [],
			"messages": [],
			"styles": [],
			"scripts": []
		}
	},
	"config_prefix": "WikiToLDAP",
	"config": {
		"LDAPMigrationGroup": {
			"value": "ldap-migration",
			"descriptionmsg": "wikitoldap-group",
			"public": true
		}
	},
	"manifest_version": 2
}
