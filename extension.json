			"LDAPAuthentication2": ">= 1.0.2"
		}
	},
	"AutoloadNamespaces": {
		"MediaWiki\\Extension\\WikiToLDAP\\": "includes/"
	},
	"Hooks": {
		"userCan": "MediaWiki\\Extension\\WikiToLDAP\\Hook::onUserCan",
		"PluggableAuthPopulateGroups":
