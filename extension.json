{
	"name": "WikiToLDAP",
	"author": [
		"Mark A. Hershberger"
	],
	"url": "https://www.mediawiki.org/wiki/Extension:WikiToLDAP",
	"descriptionmsg": "wikitoldap-desc",
	"license-name": "GPL-3.0-or-later",
	"type": "other",
	"requires": {
		"MediaWiki": ">= 1.34.0",
